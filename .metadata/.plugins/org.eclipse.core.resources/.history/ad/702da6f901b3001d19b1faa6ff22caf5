import javax.swing.JOptionPane;

public class Conversor {

	public static void main(String[] args) {


		double valorInserido;
		
		valorInserido = Integer.parseInt(JOptionPane.showInputDialog("Digite o valor:"));
		System.out.println(valorInserido);
				
		JOptionPane.showInputDialog(null, "Escolha a moeda para a qual você deseja converter", "Moedas", -1, null, "Dollar para Real" + "Outr aopção", args);
		

	}

}
